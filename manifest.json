{
  "description": "Redirects certain web pages",
  "manifest_version": 2,
  "name": "Site Block",
  "version": "0.1.2",

  "applications": {
    "gecko": {
      "id": "evans@evanshauser.com",
      "strict_min_version": "42.0",
      "update_url": "https://example.com/updates.json"
    }
  },

  "permissions": [
    "<all_urls>",
    "*://*.youtube.com/*",
    "*://*.facebook.com/*",
    "*://*.nba.com/*",
    "*://*.reddit.com/*",
    "*://*.kongregate.com/*",
    "*://*.espn.com/*",
    "*://*.go.com/*",
    "*://*.movie4k.to/*",
    "webRequest",
    "webRequestBlocking"
  ],

  "background": {
    "scripts": ["background.js"]
  }
}

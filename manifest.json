{
  "description": "Redirects certain web pages",
  "manifest_version": 2,
  "name": "Site Block",
  "version": "0.1.4",

  "applications": {
    "gecko": {
      "id": "evans@evanshauser.com",
      "strict_min_version": "42.0",
      "update_url": "https://example.com/updates.json"
    }
  },

  "permissions": ["<all_urls>", "webRequest", "webRequestBlocking"],

  "background": {
    "scripts": ["background.js"]
  }
}
